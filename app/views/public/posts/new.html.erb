<h1>新規投稿</h1>

<%= form_with model: @post, url:form_path, method: :post do |f| %>

    <div class = 'field'>
        <%= f.label :start_date, '開始日' %><br>
        <%= f.date_field :start_date %>
    </div>

    <div class = 'field'>
        <%= f.label :end_date, '終了日' %><br>
        <%= f.date_field :end_date %>
    </div>

    


        <%= f.submit '詳細入力へ' %>




<% end %>
